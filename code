set @img1
v2 0
v1 0
v4 1
label start
sprite v1 v2 1
key v3
skip if v3 not 05
exec @top
skip if v3 not 07
exec @left
skip if v3 not 08
exec @bottom
skip if v3 not 09
exec @right
jump @start
label left
sub v1 v4
return
label right
add v1 1
return
label top
sub v2 v4
return
label bottom
add v2 1
return
label img1
80
